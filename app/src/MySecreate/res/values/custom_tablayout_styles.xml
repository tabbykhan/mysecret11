<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomTabLayout">
        <attr name="defaultPaddingTop" format="dimension" />
        <attr name="defaultPaddingLeft" format="dimension" />
        <attr name="defaultPaddingRight" format="dimension" />
        <attr name="defaultPaddingBottom" format="dimension" />
        <attr name="defaultTextColor" format="color" />
        <attr name="selTextColor" format="color" />
        <attr name="capsAll" format="boolean" />
        <attr name="shouldExpand" format="boolean" />
        <attr name="defaultTextSize" format="dimension" />
        <attr name="custom_font" format="string" />
    </declare-styleable>
    <declare-styleable name="CustomTabStrip">
        <attr name="indicatorPadding" format="dimension" />
        <attr name="indicatorHeight" format="dimension" />
        <attr name="fullHeightIndicator" format="boolean" />
        <attr name="triangleIndicatorVisible" format="boolean" />
        <attr name="indicatorColor" format="color" />
        <attr name="indicatordrawable" format="reference" />
    </declare-styleable>

</resources>